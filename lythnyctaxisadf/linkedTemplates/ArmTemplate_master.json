{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "lythnyctaxisadf"
		},
		"LS_ADLSRaw_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSRaw'"
		},
		"LS_MetadataDB_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_MetadataDB'"
		},
		"LS_ADLSRaw_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://lythstorage.dfs.core.windows.net/"
		},
		"LS_GreenTaxiHTTP_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://d37ci6vzurychx.cloudfront.net"
		},
		"LS_MetadataDB_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "nyctaxissynapse.sql.azuresynapse.net"
		},
		"LS_MetadataDB_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "nyctaxissqlpool"
		},
		"LS_MetadataDB_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "synapseadmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "lythnyctaxisadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSRaw_accountKey": {
						"value": "[parameters('LS_ADLSRaw_accountKey')]"
					},
					"LS_MetadataDB_password": {
						"value": "[parameters('LS_MetadataDB_password')]"
					},
					"LS_ADLSRaw_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSRaw_properties_typeProperties_url')]"
					},
					"LS_GreenTaxiHTTP_properties_typeProperties_url": {
						"value": "[parameters('LS_GreenTaxiHTTP_properties_typeProperties_url')]"
					},
					"LS_MetadataDB_properties_typeProperties_server": {
						"value": "[parameters('LS_MetadataDB_properties_typeProperties_server')]"
					},
					"LS_MetadataDB_properties_typeProperties_database": {
						"value": "[parameters('LS_MetadataDB_properties_typeProperties_database')]"
					},
					"LS_MetadataDB_properties_typeProperties_userName": {
						"value": "[parameters('LS_MetadataDB_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}